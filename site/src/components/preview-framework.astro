import { invariant } from "@ariakit/core/utils/misc";

interface Props {
  framework?: typeof Astro.locals.framework;
}

const { framework = Astro.locals.framework } = Astro.props;

invariant(framework, "Framework is required");

const html = await Astro.slots.render(framework);
---

<Fragment set:html={html} />
