{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "plugins": [{ "name": "next" }],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "incremental": true,
    "isolatedModules": true,
    "outDir": "./dist/",
    "paths": {
      "@/*": ["./*"]
    },
    "allowImportingTsExtensions": true
  }
}
