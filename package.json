{
  "name": "reas",
  "version": "0.0.3",
  "description": "A minimalist and highly customizable component system built on top of React and styled-components",
  "license": "MIT",
  "repository": "diegohaz/reas",
  "main": "dist/index.js",
  "module": "dist/es.js",
  "jsnext:main": "dist/es.js",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish": "npm run lint && npm test && npm run build",
    "postpublish": "git push origin master --follow-tags",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test.config.js"
    ]
  },
  "keywords": [
    "reas"
  ],
  "dependencies": {
    "lodash": "^4.17.4",
    "pick-react-known-prop": "^0.1.1",
    "prop-types": "^15.5.10",
    "react-required-if": "^1.0.1",
    "recompose": "^0.26.0",
    "styled-components": "^3.0.2",
    "styled-tools": "^0.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "jest-cli": "^22.1.4",
    "opn-cli": "^3.1.0",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-icons": "^2.2.5",
    "react-styleguidist": "^6.1.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.55.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-blocks": "^1.0.0-rc.2"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
