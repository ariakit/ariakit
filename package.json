{
  "name": "reakit",
  "version": "0.13.1",
  "description": "A minimalist and highly composable UI toolkit built on top of React and styled-components",
  "license": "MIT",
  "repository": "reakit/reakit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/reakit.min.js",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src benchmark website/src scripts",
    "singel": "singel src/components/**/[A-Z]*.js -i 'src/**/*{test,Container}.js'",
    "benchmark": "node benchmark",
    "build:clean": "rimraf dist es lib && node scripts/cleanProxies",
    "prebuild": "npm run build:clean",
    "build": "rollup -c && node scripts/makeProxies",
    "dev": "styleguidist server --config styleguide.dev.config.js",
    "website": "styleguidist server",
    "website:build": "styleguidist build && cp website/dist/index.html website/dist/404.html",
    "preversion": "npm run lint && npm test && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postpublish": "npm run build:clean"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-serializer-html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/website"
    ]
  },
  "keywords": [
    "reakit",
    "react",
    "ui",
    "toolkit"
  ],
  "dependencies": {
    "constate": "^0.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.5",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-known-props": "^2.3.1",
    "styled-components": "^3.3.3",
    "styled-tools": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codemirror": "^5.39.2",
    "cross-spawn": "^6.0.5",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "jest-cli": "^23.4.2",
    "jest-serializer-html": "^5.0.0",
    "markdown-to-jsx": "^6.7.0",
    "opn-cli": "^3.1.0",
    "parse-prop-types": "^0.3.0",
    "prettier": "1.14.0",
    "pretty": "^2.0.0",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-benchmark": "^1.0.4",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.5",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "^7.1.3",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "singel": "^0.5.4",
    "standard-changelog": "^2.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-blocks": "^1.0.0-rc.2",
    "whatwg-fetch": "^2.0.4"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
