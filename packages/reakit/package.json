{
  "name": "reakit",
  "version": "0.14.0",
  "description": "A minimalist and highly composable UI toolkit built on top of React and styled-components",
  "license": "MIT",
  "repository": "reakit/reakit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/reakit.min.js",
  "types": "ts",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src scripts",
    "build:clean": "rimraf dist es lib ts && node scripts/cleanProxies",
    "build:ts": "tsc --emitDeclarationOnly --allowJs false",
    "prebuild": "npm run build:clean && npm run build:ts",
    "build": "rollup -c && node scripts/makeProxies",
    "dev": "styleguidist server",
    "preversion": "npm run lint && npm test && npm run build",
    "postversion": "cp CHANGELOG.md ../../CHANGELOG.md",
    "postpublish": "npm run build:clean"
  },
  "keywords": [
    "reakit",
    "react",
    "ui",
    "toolkit"
  ],
  "dependencies": {
    "constate": "^0.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "popper.js": "^1.14.4",
    "prop-types": "^15.6.2",
    "styled-components": "^3.4.2",
    "styled-tools": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "jest-cli": "^23.5.0",
    "opn-cli": "^3.1.0",
    "react-styleguidist": "^7.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.64.1",
    "typescript": "^3.0.1",
    "webpack": "^3.12.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
