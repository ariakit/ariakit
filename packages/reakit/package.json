{
  "name": "reakit",
  "version": "1.2.4",
  "description": "Toolkit for building accessible rich web apps with React",
  "sideEffects": false,
  "license": "MIT",
  "repository": "https://github.com/reakit/reakit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/reakit.min.js",
  "types": "ts",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/reakit"
  },
  "scripts": {
    "test": "jest",
    "coverage": "yarn test --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "clean": "node ../../scripts/build/clean.js",
    "build": "node ../../scripts/build/build.js",
    "keys": "node ../../scripts/build/keys.js",
    "docs": "node ../../scripts/build/docs.js",
    "preversion": "yarn lint && yarn test && yarn build",
    "postpublish": "yarn clean"
  },
  "keywords": [
    "reakit",
    "a11y",
    "react",
    "ui",
    "toolkit",
    "components"
  ],
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "body-scroll-lock": "^3.1.5",
    "reakit-system": "^0.14.4",
    "reakit-utils": "0.14.3",
    "reakit-warning": "^0.5.4"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "2.6.1"
  }
}
