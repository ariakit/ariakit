{
  "name": "@ariakit/playground",
  "version": "0.0.1",
  "description": "Ariakit playground",
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "homepage": "https://ariakit.org",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx"
    ],
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "exports": {
      ".": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      },
      "./*": {
        "import": "./esm/*.js",
        "require": "./cjs/*.js"
      },
      "./package.json": "./package.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariakit/ariakit.git",
    "directory": "packages/ariakit-playground"
  },
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext js,ts,tsx",
    "build": "node ../../scripts/build/build.mjs",
    "clean": "node ../../scripts/build/clean.mjs"
  },
  "keywords": [
    "ariakit",
    "react",
    "playground"
  ],
  "dependencies": {
    "@ariakit/core": "0.0.1",
    "@ariakit/react-core": "0.0.1",
    "@babel/standalone": "^7.17.2",
    "@codemirror/autocomplete": "^6.0.0",
    "@codemirror/commands": "^6.0.0",
    "@codemirror/lang-css": "^6.0.0",
    "@codemirror/lang-javascript": "^6.0.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/search": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@codesandbox/sandpack-client": "^1.3.2",
    "@emotion/react": "^11.7.1",
    "@lezer/highlight": "^1.0.0",
    "codesandbox-import-utils": "^2.2.3",
    "prismjs": "^1.26.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/babel__standalone": "7.1.4",
    "@types/prismjs": "1.26.0"
  }
}
