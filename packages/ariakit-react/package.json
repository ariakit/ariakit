{
  "name": "@ariakit/react",
  "version": "0.0.1",
  "description": "Toolkit for building accessible web apps with React",
  "sideEffects": false,
  "license": "MIT",
  "homepage": "https://ariakit.org",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "exports": {
      ".": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      },
      "./*": {
        "import": "./esm/*.js",
        "require": "./cjs/*.js"
      },
      "./package.json": "./package.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariakit/ariakit.git",
    "directory": "packages/ariakit-react"
  },
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/ariakit"
  },
  "scripts": {
    "lint": "eslint . --ext js,ts,tsx",
    "build": "node ../../scripts/build/build.mjs",
    "clean": "node ../../scripts/build/clean.mjs"
  },
  "keywords": [
    "ariakit",
    "react",
    "a11y",
    "ui",
    "toolkit",
    "components"
  ],
  "dependencies": {
    "@ariakit/core": "0.0.1",
    "@ariakit/react-core": "0.0.1",
    "@floating-ui/dom": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
