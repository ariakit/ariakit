{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node16",
    "strictNullChecks": true
  },
  "include": [
    "**/*.tsx",
    "**/*.ts",
    ".pages/**/*.ts",
    ".next/types/**/*.ts"
  ]
}
