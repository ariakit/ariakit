{
  "name": "@ariakit/test",
  "version": "0.0.1",
  "description": "Ariakit test utils",
  "license": "MIT",
  "homepage": "https://ariakit.org",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "exports": {
      ".": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      },
      "./*": {
        "import": "./esm/*.js",
        "require": "./cjs/*.js"
      },
      "./__chunks/*": null,
      "./package.json": "./package.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariakit/ariakit.git",
    "directory": "packages/ariakit-test"
  },
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "scripts": {
    "lint": "eslint . --ext js,ts,tsx",
    "build": "node ../../scripts/build/build.mjs",
    "clean": "node ../../scripts/build/clean.mjs"
  },
  "keywords": [
    "ariakit",
    "react"
  ],
  "dependencies": {
    "@ariakit/core": "0.0.1"
  },
  "peerDependencies": {
    "@testing-library/dom": "^8.0.0",
    "@testing-library/react": "^12.0.0 || ^13.0.0 || ^13.0.0-alpha",
    "react": "^17.0.0 || ^18.0.0"
  }
}
